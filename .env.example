# Environment Variables Template
# Copy this file to .env and fill in your actual API keys

# Alpha Vantage API (for stock data)
# Get your free API key at: https://www.alphavantage.co/support/#api-key
ALPHA_VANTAGE_API_KEY=your_alpha_vantage_api_key_here

# Twelve Data API (optional, for additional stock data)
# Get your free API key at: https://twelvedata.com/
TWELVE_DATA_API_KEY=your_twelve_data_api_key_here

# Financial Modeling Prep API (optional)
# Get your free API key at: https://financialmodelingprep.com/
FMP_API_KEY=your_fmp_api_key_here

# CoinGecko Pro API (optional, for higher rate limits)
COINGECKO_PRO_API_KEY=your_coingecko_pro_api_key_here

# Firebase Configuration
FIREBASE_API_KEY=your_firebase_api_key_here
FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
FIREBASE_PROJECT_ID=your_project_id
FIREBASE_STORAGE_BUCKET=your_project.firebasestorage.app
FIREBASE_MESSAGING_SENDER_ID=your_sender_id
FIREBASE_APP_ID=your_app_id
FIREBASE_MEASUREMENT_ID=your_measurement_id

# Development/Production Environment
NODE_ENV=development

# App Configuration
APP_VERSION=1.0.0
DEBUG_MODE=true
